{
  "name": "@qforge/torque",
  "version": "0.1.3",
  "description": "Like React, but for datasets - A declarative, typesafe DSL for building LLM training datasets",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "llm",
    "dataset",
    "training-data",
    "fine-tuning",
    "ai",
    "machine-learning",
    "declarative",
    "dsl",
    "typescript",
    "zod"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qforge-dev/torque.git",
    "directory": "builder"
  },
  "bugs": {
    "url": "https://github.com/qforge-dev/torque/issues"
  },
  "homepage": "https://github.com/qforge-dev/torque/tree/main/builder#readme",
  "peerDependencies": {
    "ai": "^5.0.0",
    "zod": "^4.0.0"
  },
  "dependencies": {
    "tiktoken": "^1.0.22",
    "zod-from-json-schema": "0.5.1"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^2.0.58",
    "@types/bun": "latest",
    "bun-types": "latest",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "bun run clean && bun run compile",
    "clean": "rm -rf dist",
    "compile": "tsc -p tsconfig.build.json",
    "generate:templates": "bun run scripts/generate-stackblitz-templates.ts",
    "prebuild": "bun run generate:templates",
    "prepublishOnly": "bun run build",
    "prepack": "bun run build"
  }
}
